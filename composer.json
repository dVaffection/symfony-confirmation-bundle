    "keywords":    [
        "symfony", "confirmation"
    ],
    "homepage":    "https://github.com/dVaffection/symfony-confirmation-bundle",
    "require":     {
        "php":                                ">=5.3.3",
        "dv-affection/abstract-confirmation": "~0.0"
